pipeline {

    agent any
    environment {
        PATH = "/usr/share/maven/bin:$PATH"
    }
    stages {
        stage('CHECKOUT GIT') {

            steps{
                echo 'Pulling...';
                git branch: 'main',
                url: 'https://github.com/nadaTLILI/ProjetDevops.git',
                credentialsId: 'gitAmina';
            }
        }
        stage('BUILD') {
            steps{
                sh 'mvn -f DevopsBackendProject/pom.xml clean install'
                }
        }

    }


}